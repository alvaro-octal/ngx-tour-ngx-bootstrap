{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "importHelpers": true,
    "module": "es2020",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "paths": {
      "@ngx-tour/core": ["projects/ngx-tour-core/dist"],
      "@ngx-tour/console": ["projects/ngx-tour-console/dist"],
      "@ngx-tour/md-menu": ["projects/ngx-tour-md-menu/dist"],
      "@ngx-tour/ng-bootstrap": ["projects/ngx-tour-ng-bootstrap/dist"],
      "@ngx-tour/ngx-bootstrap": ["projects/ngx-tour-ngx-bootstrap/dist"],
      "@ngx-tour/ngx-popper": ["projects/ngx-tour-ngx-popper/dist"]
    }
  }
}
